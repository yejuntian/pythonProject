<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<properties>
    <!--需要手动还原ConstraintLayout资源属性-->

    <!--水平和垂直方向的定位-->
    <comment>layout_constraintLeft_toLeftOf</comment>
    <comment>layout_constraintLeft_toRightOf</comment>
    <comment>layout_constraintRight_toLeftOf</comment>
    <comment>layout_constraintRight_toRightOf</comment>
    <comment>layout_constraintTop_toTopOf</comment>
    <comment>layout_constraintTop_toBottomOf</comment>
    <comment>layout_constraintBottom_toTopOf</comment>
    <comment>layout_constraintBottom_toBottomOf</comment>
    <comment>layout_constraintStart_toStartOf</comment>
    <comment>layout_constraintStart_toEndOf</comment>
    <comment>layout_constraintEnd_toStartOf</comment>
    <comment>layout_constraintEnd_toEndOf</comment>
    <comment>layout_constraintBaseline_toBaselineOf</comment>

    <!--Bias（偏移）-->
    <comment>layout_constraintHorizontal_bias</comment>
    <comment>layout_constraintVertical_bias</comment>

    <!--百分比尺寸-->
    <comment>layout_constraintWidth_percent</comment>
    <comment>layout_constraintHeight_percent</comment>

    <!--宽高比例-->
    <comment>layout_constraintDimensionRatio</comment>

    <!--引导线-->
    <comment>layout_constraintGuide_begin</comment>
    <comment>layout_constraintGuide_end</comment>
    <comment>layout_constraintGuide_percent</comment>

    <!--GONE 状态下的边距-->
    <!--<comment>layout_goneMarginLeft</comment>-->
    <comment>layout_goneMarginTop</comment>
    <comment>layout_goneMarginRight</comment>
    <comment>layout_goneMarginBottom</comment>
    <comment>layout_goneMarginEnd</comment>

    <!--链式布局-->
    <comment>layout_constraintHorizontal_chainStyle</comment>
    <comment>layout_constraintVertical_chainStyle</comment>
    <comment>layout_constraintHorizontal_weight</comment>
    <!--<comment>layout_constraintVertical_weight</comment>-->

    <!--圆-->
    <comment>layout_constraintCircle</comment>
    <comment>layout_constraintCircleRadius</comment>
    <comment>layout_constraintCircleAngle</comment>

    <!--最小和最大尺寸-->
    <!--<comment>layout_constraintWidth_min</comment>-->
    <comment>layout_constraintHeight_min</comment>
    <comment>layout_constraintWidth_max</comment>
    <!--<comment>layout_constraintHeight_max</comment>-->

    <!--指定视图的高度约束的默认行为，默认值为 wrap_content，这表示视图的高度会根据其内容自动调整-->
    <comment>layout_constraintHeight_default</comment>
    <comment>layout_constraintWidth_default</comment>

    <!--layout_constrainedHeight 属性被设置为 true 时，它表示该视图的高度应该受到某些约束的限制，而不是自由地扩展到所需的高度。这些约束可能来自于父级容器的大小限制，或者是通过布局系统中的其他方式定义的-->
    <comment>layout_constrainedHeight</comment>

    <!-- 允许在 ConstraintLayout 中指定一个或多个视图的 ID，用于定义约束布局中视图之间的关系-->
    <comment>constraint_referenced_ids</comment>
    <!--用于指定 Barrier 控件的方向-->
    <comment>barrierDirection</comment>

</properties>
